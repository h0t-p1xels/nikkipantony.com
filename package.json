	"engines": {
		"node": ">= 16.16.0",
		"yarn": "^1.22.19"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"scripts": {
		"dev-g": "gatsby develop -p 8008",
		"dev-s": "start-storybook -p 6006",
		"dev-d": "start-storybook --docs -p 5005",
		"build-g": "gatsby build",
		"build-s": "build-storybook -o ./public-storybook",
		"build-d": "build-storybook --docs -o ./public-design-system",
		"serve-g": "http-server ./public -p 8080",
		"serve-s": "http-server ./public-storybook -p 8081",
		"serve-d": "http-server ./public-design-system -p 8082",
		"gen-dt": "sass-export src/0_Tokens/**/*.scss -o ./design-tokens.json",
		"new-atom": "hygen atom new --name",
		"new-mol": "hygen molecule new --name",
		"new-org": "hygen organism new --name",
		"new-temp": "hygen template new --name",
		"new-page": "hygen page new --name",
		"new-guide": "hygen guide new --name",
		"start": "npm run develop",
		"build": "gatsby build",
		"develop": "gatsby develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"storybook": "NODE_ENV=production start-storybook -p 6006 -s static",
		"build-storybook": "NODE_ENV=production build-storybook -s static"
	},
	"dependencies": {
		"@fontsource/ibm-plex-mono": "^4.5.10",
